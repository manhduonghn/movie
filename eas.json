{
  "cli": {
    "version": ">= 3.20.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "image": "latest"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "image": "latest"
      }
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "image": "latest"
      }
    },
    "sandbox": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "image": "latest",
        "buildType": "apk"
      },
      "env": {
        "EAS_SKIP_AUTO_FINGERPRINT": "1",
        "EXPO_NO_METRO": "1"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
